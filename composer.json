{
    "name": "horde/lock",
    "description": "Resource locking library",
    "type": "library",
    "homepage": "https://www.horde.org/libraries/Horde_Lock",
    "license": "LGPL-2.1-only",
    "authors": [
        {
            "name": "Ben Klang",
            "email": "ben@alkaloid.net",
            "role": "lead"
        }
    ],
    "time": "2022-11-04",
    "repositories": [
        {
            "type": "composer",
            "url": "https://horde-satis.maintaina.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8",
        "horde/exception": "^3 || dev-FRAMEWORK_6_0",
        "horde/support": "^3 || dev-FRAMEWORK_6_0"
    },
    "require-dev": {},
    "suggest": {
        "horde/db": "^3 || dev-FRAMEWORK_6_0",
        "horde/log": "^3 || dev-FRAMEWORK_6_0",
        "horde/mongo": "^2 || dev-FRAMEWORK_6_0"
    },
    "autoload": {
        "psr-0": {
            "Horde_Lock": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Horde\\Lock\\Test\\": "test/"
        }
    },
    "config": {
        "allow-plugins": {
            "horde/horde-installer-plugin": true
        }
    }
}